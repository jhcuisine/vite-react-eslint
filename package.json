    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "less": "^4.2.0",
    "lightningcss": "^1.27.0",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.80.5",
    "stylus": "^0.64.0",
    "taze": "^0.16.3",
    "vite": "^5.3.5"
