    "format": "prettier . --write",
    "formatcheck": "prettier . --check",
    "list": "eslint --print-config eslint.config.js",
    "time": "set TIMING=all && npx eslint **/*.{js,jsx} --stats -f mo",
    "timefix": "set TIMING=all && npx eslint **/*.{js,jsx} --fix --stats -f mo"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
